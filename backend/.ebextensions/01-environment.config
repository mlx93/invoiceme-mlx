option_settings:
  aws:elasticbeanstalk:application:environment:
    # Server Configuration (required for Elastic Beanstalk)
    SERVER_PORT: '5000'
    SPRING_PROFILES_ACTIVE: 'production'
    
    # Logging
    LOGGING_LEVEL_ROOT: 'INFO'
    LOGGING_LEVEL_COM_INVOICEME: 'INFO'
    
# Note: Environment variables (DATABASE_URL, JWT_SECRET, AWS_ACCESS_KEY_ID, etc.)
# should be configured via Elastic Beanstalk Console:
# Configuration -> Software -> Environment properties
# This file only sets application-level configuration that doesn't contain secrets.

